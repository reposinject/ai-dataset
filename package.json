  "config": {
    "dataset_type": "ai-optimization",
    "industry": "telecommunications",
    "service_type": "wireless-internet-service-provider",
    "target_market": "business-enterprise",
    "geographic_scope": "united-states",
    "specialization": "underserved-rural-markets"
  }
}