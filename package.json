  },
  "files": [
    "ai-dataset.json",
    "ai-dataset-core.json", 
    "ai-dataset-expansion.json",
    "llm.txt",
    "robots.txt"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "validate": "node -e \"console.log('Validating JSON files...'); require('./ai-dataset.json'); require('./ai-dataset-core.json'); require('./ai-dataset-expansion.json'); console.log('All JSON files valid');\""
